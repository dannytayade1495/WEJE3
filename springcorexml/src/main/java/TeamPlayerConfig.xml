<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans 
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.jspiders.springcorexml.beans.TeamBean"
		name="team1">
		<property name="id" value="1" />
		<property name="name" value="CSK" />
		<property name="city" value="Chennai" />
		<property name="players">
			<list>
				<bean class="com.jspiders.springcorexml.beans.PlayerBean"
					name="player1">
					<property name="id" value="1" />
					<property name="name" value="M S Dhoni" />
					<property name="role" value="WK/Bat" />
					<property name="jersey_no" value="7" />
				</bean>
				<bean class="com.jspiders.springcorexml.beans.PlayerBean"
					name="player2">
					<property name="id" value="2" />
					<property name="name" value="Jadeja" />
					<property name="role" value="All-rounder" />
					<property name="jersey_no" value="8" />
				</bean>
			</list>
		</property>
	</bean>

</beans>